    <output file="gfxdata/tiles.inc">
        <tiles var-name="tiles" />
        <maps pointers-size="8">
            <map var-name="mapHigatanaStill" left="0" top="2" width="2" height="2" />
            <map var-name="mapHigatanaA" left="2" top="2" width="2" height="2" />
            <map var-name="mapHigatanaB" left="4" top="2" width="2" height="2" />
            <map var-name="mapHigatanaC" left="6" top="2" width="2" height="2" />
        </maps>
    </output>
</gfx-xform>
